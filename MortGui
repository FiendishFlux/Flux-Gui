	Name = "Loop All",
	Callback = function()
      	game.Players:Chat("/lka")
  	end    
})




Tab1:AddLabel("Hidden Loop")
local player = game.Players.LocalPlayer

local hiddenloop = false
Tab1:AddToggle({
	Name = "Toggle",
	Default = false,
	Callback = function(Value)
		hiddenloop = Value
	end    
})
spawn(function()
    while true do
        pcall(function()
            player.Character.ArmAngle.Disabled = hiddenloop
            wait(0.3)
        end)
    end
end)


